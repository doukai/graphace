{
	"name": "graphence",
	"version": "0.0.1",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./i18n/export.json": "./dist/i18n/export.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "tsx src/lib/i18n/import.ts && houdini pull-schema && graphql-codegen && svelte-kit sync",
		"dev": "concurrently --kill-others \"vite dev\" \"typesafe-i18n\"",
		"build": "tsx src/lib/i18n/import.ts && typesafe-i18n --no-watch && vite build",
		"build:package": "tsx src/lib/i18n/export.ts && vite build && npm run package",
		"package": "svelte-kit sync && svelte-package && publint",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && TIMING=1 eslint \"src\"",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"generate": "concurrently --kill-others \"houdini generate\" \"graphql-codegen\" \"typesafe-i18n\"",
		"graphql:codegen": "graphql-codegen",
		"houdini:generate": "houdini generate",
		"houdini:pull-schema": "houdini pull-schema",
		"i18n": "typesafe-i18n",
		"i18n:import": "tsx src/lib/i18n/import.ts",
		"i18n:export": "tsx src/lib/i18n/export.ts"
	},
	"dependencies": {
		"@graphace/commons": "workspace:*",
		"@graphace/graphql": "workspace:*",
		"@graphace/ui": "workspace:*",
		"@graphace/ui-graphql": "workspace:*"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.3.1",
		"@graphql-codegen/introspection": "^3.0.1",
		"@graphql-codegen/typescript": "^3.0.4",
		"@iconify/svelte": "^3.1.6",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.83.0",
		"@revolist/revogrid-column-numeral": "^1.0.2",
		"@revolist/svelte-datagrid": "<4.10.34",
		"@steeze-ui/heroicons": "^2.3.0",
		"@steeze-ui/svelte-icon": "~1.4.1",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-node": "^2.1.2",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/typography": "^0.5.13",
		"@types/lodash": "^4.17.6",
		"@types/node": "^18.19.39",
		"ajv": "^8.16.0",
		"ajv-formats": "^2.1.1",
		"ajv-i18n": "^4.2.0",
		"autoprefixer": "^10.4.19",
		"chart.js": "^4.4.4",
		"chartjs-plugin-autocolors": "^0.2.2",
		"concurrently": "^8.2.2",
		"daisyui": "^3.9.4",
		"date-fns": "^2.30.0",
		"dotenv": "^16.4.5",
		"eslint-config-custom": "workspace:*",
		"graphace-codegen": "workspace:*",
		"graphace-codegen-preset": "workspace:*",
		"graphql": "^16.9.0",
		"houdini": "^1.2.64",
		"houdini-svelte": "^1.2.64",
		"jwt-decode": "^3.1.2",
		"lodash": "^4.17.21",
		"nanoid": "^4.0.2",
		"postcss": "^8.4.39",
		"postcss-load-config": "^3.1.4",
		"publint": "^0.1.16",
		"svelte": "^4.2.19",
		"svelte-boring-avatars": "^1.2.6",
		"svelte-chartjs": "^3.1.5",
		"svelte-check": "^4.0.2",
		"svelte-preprocess": "^5.1.4",
		"svelte-typeahead": "^4.4.1",
		"tailwindcss": "^3.4.4",
		"theme-change": "^2.5.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.3",
		"tsx": "^4.16.0",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.1.6",
		"vite": "^5.4.10",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
	},
	"peerDependencies": {
		"@sveltejs/kit": ">=2.0.0",
		"svelte": ">=4.0.0"
	}
}