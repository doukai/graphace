    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/kit": "latest",
    "eslint-config-custom": "workspace:*",
    "tailwind-config": "workspace:*",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "vite": "^3.1.6",
    "@iconify/svelte": "^2.2.1",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "concurrently": "^7.1.0",
    "theme-change": "^2.0.2",
