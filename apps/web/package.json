{
  "name": "@graphace/web",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently --kill-others \"vite dev\" \"typesafe-i18n\"",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path=../../.prettierignore . && TIMING=1 eslint \"src\"",
    "format": "prettier --write --ignore-path=../../.prettierignore .",
    "generate": "concurrently --kill-others \"houdini generate\" \"graphql-codegen\" \"typesafe-i18n\"",
    "houdini-generate": "houdini generate",
    "graphql-codegen": "graphql-codegen",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@graphace/graphql": "workspace:*",
    "@graphace/ui": "workspace:*",
    "@graphace/ui-graphql": "workspace:*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@iconify/svelte": "^2.2.1",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^18.11.9",
    "@types/lodash": "^4.14.182",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "daisyui": "^2.39.0",
    "eslint-config-custom": "workspace:*",
    "graphace-codegen": "workspace:*",
    "graphace-codegen-preset": "workspace:*",
    "graphql": "^16.6.0",
    "houdini": "^0.20.3",
    "houdini-svelte": "^0.20.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "theme-change": "^2.0.2",
    "ts-node": "^10.9.1",
		"tslib": "^2.4.1",
    "typesafe-i18n": "5.3.6",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
  },
  "type": "module"
}