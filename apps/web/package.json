{
  "name": "@graphace/web",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --port 3000 --open",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
