{
	"name": "@graphace/ui-graphql",
	"version": "0.1.1",
	"description": "Graphql components for use in svelte websites",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./i18n/export.json": "./dist/i18n/export.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsx src/lib/i18n/export.ts && vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"i18n": "typesafe-i18n",
		"i18n:export": "tsx src/lib/i18n/export.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"@sveltejs/package": "^2.3.2",
		"eslint-config-custom": "workspace:*",
		"publint": "^0.1.16",
		"svelte": "^3.59.2",
		"svelte-check": "^3.8.4",
		"tslib": "^2.6.3",
		"tsx": "^4.16.0",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^4.9.5",
		"vite": "^4.5.3"
	},
	"dependencies": {
		"@graphace/commons": "workspace:*",
		"@graphace/graphql": "workspace:*",
		"@graphace/ui": "workspace:*"
	},
	"peerDependencies": {
		"@iconify/svelte": ">=3.1.0",
		"@melt-ui/pp": ">=0.3.0",
		"@melt-ui/svelte": ">=0.83.0",
		"@steeze-ui/heroicons": ">=2.3.0",
		"@steeze-ui/svelte-icon": ">=1.4.0",
		"@sveltejs/kit": ">=1.30.0",
		"nanoid": ">=4.0.0",
		"svelte": ">=3.59.0"
	},
	"publishConfig": {
		"access": "public"
	}
}