{
	"name": "@graphace/ui",
	"version": "0.0.13",
	"description": "Components for use in svelte websites",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./i18n/export.json": "./dist/i18n/export.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf dist",
		"build": "tsx src/lib/i18n/export.ts && vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"i18n:export": "tsx src/lib/i18n/export.ts",
		"typesafe-i18n": "typesafe-i18n",
		"publish:yalc": "yalc publish"
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.2",
		"@sveltejs/package": "^2.0.0",
		"publint": "^0.1.9",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.3",
		"vite": "^4.3.9",
		"typescript": "^4.9.5",
		"tsx": "^3.12.7",
		"typesafe-i18n": "^5.24.3"
	},
	"dependencies": {
		"@graphace/commons": "workspace:*",
		"tippy.js": "^6.3.7"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.20.2",
		"@sveltejs/package": "^2.0.0",
		"@iconify/svelte": "^3.1.4",
		"@steeze-ui/heroicons": "^2.2.2",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"svelte-typeahead": "^4.4.1",
		"theme-change": "^2.5.0",
		"date-fns": "^2.30.0",
		"nanoid": "^4.0.2",
		"svelte": "^3.59.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
