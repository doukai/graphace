[
  {
    "name": "Objects",
    "icon": "material-symbols:data-object",
    "items": [
{%- if objects %}
{%- for object in objects %}
	    {
        "name": "{{ object.name }}",
        "tags": "{{ object.name }}",
        "href": "/{{ object.name | paramCase }}"{%- if useAuth %},
        "permissions": ["{{ object.name }}::*::*"]{%- endif %}
	    }{%- if forloop.last == false -%},{%- endif %}
{%- endfor %}
{%- endif %}
    ]
  }
]