{
	"name": "graphace",
	"version": "0.0.1",
	"description": "The user interface for Graphoenix",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"build:packages": "turbo run build --filter=./packages/*",
		"publish:local": "pnpm publish --no-git-checks --registry http://localhost:4873/ --filter=./packages/*",
		"publish:yalc": "turbo run publish:yalc --filter=./packages/*",
		"daemon:clean": "turbo daemon clean",
		"login": "turbo login",
		"clean": "npx npkill",
		"clean:dist": "turbo run clean",
		"lint": "turbo run lint",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"eslint": "^8.42.0",
		"eslint-config-custom": "workspace:*",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.1",
		"turbo": "^1.10.3"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "pnpm@8.6.2"
}
