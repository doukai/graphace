{
  "name": "graphace",
  "version": "0.0.1",
  "description": "The user interface for Graphoenix",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
