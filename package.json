{
  "name": "graphace",
  "version": "0.0.1",
  "description": "The user interface for Graphoenix",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "nohoist": [
      "**/typesafe-i18n",
      "**/typesafe-i18n/**"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "daemon-clean": "turbo daemon clean",
    "login": "turbo login",
    "clean": "npx npkill",
    "lint": "turbo run lint",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.1",
    "turbo": "^1.10.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@8.6.2"
}