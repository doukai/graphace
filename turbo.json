{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", ".vercel/**"]
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"publish:yalc": {
			"dependsOn": ["^publish:yalc"]
		},
		"clean:yalc": {
			"dependsOn": ["^clean:yalc"]
		}
	}
}
